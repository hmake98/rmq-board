{
    "name": "@rmq-board/backend",
    "version": "1.1.0",
    "description": "A powerful and intuitive admin UI for RabbitMQ with direct AMQP support and real-time monitoring",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"**/*.{js,json}\"",
        "debug": "node --inspect server.js",
        "start:prod": "NODE_ENV=production node server.js"
    },
    "keywords": [
        "rabbitmq",
        "admin",
        "ui",
        "dashboard",
        "monitoring",
        "amqp",
        "message-queue",
        "real-time"
    ],
    "author": "hmake98",
    "license": "MIT",
    "dependencies": {
        "amqplib": "^0.10.3",
        "axios": "^1.4.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-rate-limit": "^6.9.0",
        "helmet": "^7.0.0",
        "socket.io": "^4.7.1",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    }
}